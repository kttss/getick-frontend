{
  "name": "getick",
  "version": "0.0.1",
  "license": "",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start-hmr": "ng serve --configuration hmr --source-map=false --hmr-warning=false",
    "start-hmr-sourcemaps": "ng serve --configuration hmr --source-map=true --hmr-warning=false",
    "build": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev",
    "build-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev --stats-json",
    "build-prod": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod",
    "build-prod-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "validate:quick": "npm run lint:config && lint-staged",
    "lint:config": "tslint-config-prettier-check ./tslint.json"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.0.0",
    "@angular/animations": "11.2.3",
    "@angular/cdk": "8.0.0",
    "@angular/common": "11.2.3",
    "@angular/compiler": "11.2.3",
    "@angular/core": "11.2.3",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "11.2.3",
    "@angular/material": "8.0.0",
    "@angular/material-moment-adapter": "8.0.0",
    "@angular/platform-browser": "11.2.3",
    "@angular/platform-browser-dynamic": "11.2.3",
    "@angular/router": "11.2.3",
    "@commitlint/cli": "12.0.1",
    "@ngrx/effects": "8.0.0-beta.2",
    "@ngrx/router-store": "8.0.0-beta.2",
    "@ngrx/store": "8.0.0-beta.2",
    "@ngrx/store-devtools": "8.0.0-beta.2",
    "@ngx-translate/core": "13.0.0",
    "@swimlane/dragula": "3.8.0",
    "@swimlane/ngx-charts": "11.1.0",
    "@swimlane/ngx-datatable": "15.0.0",
    "@swimlane/ngx-dnd": "7.0.0",
    "@types/prismjs": "1.16.0",
    "angular-calendar": "0.28.20",
    "angular-in-memory-web-api": "0.11.0",
    "chart.js": "2.9.4",
    "classlist.js": "1.1.20150312",
    "d3": "5.9.2",
    "date-fns": "2.18.0",
    "hammerjs": "2.0.8",
    "husky": "4.3.8",
    "i": "0.3.6",
    "lint-staged": "10.5.4",
    "lodash": "4.17.21",
    "moment": "2.24.0",
    "ng-lint-staged": "0.1.6",
    "ng2-charts": "2.2.4",
    "ngrx-store-freeze": "0.2.4",
    "ngx-color-picker": "7.5.0",
    "ngx-cookie-service": "2.2.0",
    "npm": "7.6.0",
    "perfect-scrollbar": "1.4.0",
    "prettier": "2.2.1",
    "prismjs": "1.16.0",
    "rxjs": "6.6.6",
    "tslib": "^2.0.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular/cli": "11.2.2",
    "@angular/compiler-cli": "11.2.3",
    "@angular/language-service": "11.2.3",
    "@angularclass/hmr": "3.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash": "4.14.132",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "7.0.1",
    "tslint": "~6.1.0",
    "typescript": "4.0.7",
    "webpack-bundle-analyzer": "3.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "./scripts/post-checkout.sh $HUSKY_GIT_PARAMS",
      "post-merge": "npm ci"
    }
  },
  "lint-staged": {
    "./**/*.{js,ts}": [
      "ng-lint-staged lint --fix --",
      "git add",
      "prettier --write",
      "git add"
    ],
    "./**/*.{css,scss}": [
      "prettier --write",
      "git add"
    ],
    "./**/*.{json,md,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
